	"commands": {
		"filesystem": {
			"command": "df -h", 
			"fields": ["Filesystem", "Size", "Mounted"]
		},
		"blockdev": {
			"command": "sudo blockdev --report", 
			"fields": ["RA", "Device"]
		},
		"journaling": {
			"command": "sudo dumpe2fs /dev/sda1 | grep journal",
			"fields": []
		},
		"IOplanner": {
			"command": "cat /sys/block/xvdb/queue/scheduler ",
			"fields": []
		},
		"ports": {
			"command": "sudo lsof -i",
			"fields": ["COMMAND", "USER"]
		},
		"java": {
			"command": "java -version",
			"fields": []
		},
		"largePages": {
			"command": "cat /proc/meminfo | grep Huge",
			"fields": ["Hugepagesize"]
		},
		"raid": {
			"command": "cat /proc/mdstat",
			"fields": []
		}
	},
	"desiredSettings": {
		"java": "java version 1.7.0_55",
		"IOplanner": "noop",
		"blockdev": {"RA": "256"},
		"journaling": "error",
		"filesystem": {"Filesystem": ["devtmpfs", "error"]}
	}
}